{
	"name": "rty-stf-po",
	"version": "0.0.0-beta.112",
	"scripts": {
		"ng": "ng",
		"start": "ng serve --host local.dev.egsdlon.dk --port 80 --prod --base-href /egsdloen/ --deploy-url /egsdloen/",
		"start-secure": "ng serve --prod --host fer.rock.sfk.cv --port 443 --ssl true --ssl-key \"ssl/server.key\" --ssl-cert \"ssl/server.crt\"",
		"build": "ng build",
		"test": "ng test --sourceMap=false",
		"test-cc": "ng test --watch=false --sourceMap=false --code-coverage",
		"test-no-watch": "ng test --watch=false --sourceMap=false",
		"test-headless": "ng test --sourceMap=false --watch=false --browsers ChromeHeadless",
		"test-headless-cc": "ng test --sourceMap=false --watch=false --browsers ChromeHeadless --code-coverage",
		"lint": "ng lint rty-stf-po --fix",
		"lint-nofix": "ng lint rty-stf-po",
		"e2e": "npx cypress run",
		"e2e-debug": "npx cypress open",
		"JENKINS-TEST": "npm run lint-nofix && npm run test-headless-cc",
		"JENKINS-BUILD": "ng build --configuration=production --prod --base-href /steam/ --deploy-url /steam/ ",
		"JENKINS-SERVE": "ng serve --configuration=production --prod --base-href /steam/ --deploy-url /steam/",
		"bundle-report": "ng build --configuration=production --prod --stats-json && webpack-bundle-analyzer dist/stats.json"
	},
	"dependencies": {
		"@angular/animations": "6.1.7",
		"@angular/cdk": "6.4.7",
		"@angular/cli": "6.2.2",
		"@angular/common": "6.1.7",
		"@angular/compiler": "6.1.7",
		"@angular/core": "6.1.7",
		"@angular/forms": "6.1.7",
		"@angular/http": "6.1.7",
		"@angular/material": "6.4.7",
		"@angular/platform-browser": "6.1.7",
		"@angular/platform-browser-dynamic": "6.1.7",
		"@angular/router": "6.1.7",
		"@datorama/akita": "1.7.2",
		"@swimlane/ngx-datatable": "13.1.0",
		"angular-l10n": "5.2.0",
		"classlist.js": "1.1.20150312",
		"core-js": "2.5.7",
		"less": "3.8.1",
		"moment": "2.22.2",
		"ngx-mydatepicker": "2.4.9",
		"npm": "6.4.1",
		"rxjs": "6.3.2",
		"web-animations-js": "2.3.1",
		"whatwg-fetch": "3.0.0",
		"zone.js": "0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "0.7.2",
		"@angular/compiler-cli": "6.1.7",
		"@angular/language-service": "6.1.7",
		"@bahmutov/add-typescript-to-cypress": "2.0.0",
		"@types/jasmine": "2.8.8",
		"@types/jasminewd2": "2.0.3",
		"@types/node": "10.10.0",
		"codelyzer": "4.4.4",
		"cypress": "3.1.0",
		"jasmine-core": "3.2.1",
		"karma": "3.0.0",
		"karma-chrome-launcher": "2.2.0",
		"karma-coverage-istanbul-reporter": "2.0.4",
		"karma-jasmine": "1.1.2",
		"karma-jasmine-html-reporter": "1.3.1",
		"karma-junit-reporter": "1.2.0",
		"ts-node": "7.0.1",
		"tslint": "5.11.0",
		"typescript": "2.9.2",
		"webpack-bundle-analyzer": "3.0.2"
	}
}
